  "version": "1.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://prachwal.github.io/my-react-app",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "clean": "rimraf dist",
    "rebuild": "npm run clean && npm run build",
    "test": "jest",
    "docker:build": "docker build -t my-react-app .",
    "docker:start": "docker-compose up --build",
    "lint": "npx eslint --config eslint.config.js \"src/**/*.{ts,tsx}\"",
    "lint:fix": "npx eslint --config eslint.config.js \"src/**/*.{ts,tsx}\" --fix",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
